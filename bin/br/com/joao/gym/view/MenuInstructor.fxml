<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="630.0" prefWidth="700.0" style="-fx-background-color: #787878;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.joao.gym.view.MenuInstructorController">
   <children>
      <Label layoutX="60.0" layoutY="40.0" text="Instructor Menu" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <GridPane layoutX="60.0" layoutY="104.0" prefHeight="159.0" prefWidth="182.0" vgap="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleViewExistingSeries" prefHeight="30.0" prefWidth="170.0" text="View Existing Series" GridPane.rowIndex="1">
               <tooltip>
                  <Tooltip text="Click to create a new series">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Tooltip>
               </tooltip>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleCreateNewSeries" prefHeight="30.0" prefWidth="170.0" text="Create New Series">
               <tooltip>
                  <Tooltip text="Click to view existing series/perform workout ">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Tooltip>
               </tooltip>
            </Button>
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="187.0" minWidth="10.0" prefWidth="134.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <Label fx:id="userNameLabel" layoutX="543.0" layoutY="45.0" text="userNameLabel" textFill="WHITE" />
   </children>
</AnchorPane>
