<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="630.0" prefWidth="700.0" style="-fx-background-color: #787878;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.joao.gym.view.MenuReceptionistController">
   <children>
      <Label layoutX="60.0" layoutY="40.0" text="Recepcionist Menu" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font></Label>
      <GridPane layoutX="60.0" layoutY="104.0" prefHeight="159.0" prefWidth="341.0" vgap="20.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="187.0" minWidth="10.0" prefWidth="187.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="165.0" minWidth="10.0" prefWidth="154.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints vgrow="SOMETIMES" />
          <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#handleRegisterNewMember" prefHeight="30.0" prefWidth="170.0" text="Register New Member">
               <font>
                  <Font size="14.0" />
               </font>
               <tooltip>
                  <Tooltip text="Click to register a new member">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Tooltip>
               </tooltip></Button>
            <Button mnemonicParsing="false" onAction="#handleVerifyPayment" prefHeight="30.0" prefWidth="170.0" text="Verify Payment" GridPane.rowIndex="1">
               <font>
                  <Font size="14.0" />
               </font>
               <tooltip>
                  <Tooltip text="Click to verify a member">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Tooltip>
               </tooltip></Button>
            <Button mnemonicParsing="false" onAction="#handleScheduleEvaluation" prefHeight="30.0" prefWidth="170.0" text="Schedule Evaluation" GridPane.rowIndex="2">
               <font>
                  <Font size="14.0" />
               </font>
               <tooltip>
                  <Tooltip text="Click to schedule an evaluation">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Tooltip>
               </tooltip>
            </Button>
         </children>
      </GridPane>
      <Label fx:id="userNameLabel" layoutX="543.0" layoutY="45.0" text="userNameLabel" textFill="WHITE" />
   </children>
</AnchorPane>
